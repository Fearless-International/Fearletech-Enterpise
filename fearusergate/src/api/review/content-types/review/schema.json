{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "singularName": "review",
    "pluralName": "reviews",
    "displayName": "Review"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "Rating": {
      "type": "integer",
      "required": true,
      "max": 5,
      "min": 1
    },
    "Comment": {
      "type": "text",
      "required": true
    },
    "user": {
      "type": "component",
      "repeatable": false,
      "component": "shared.user-info",
      "required": true
    },
    "service": {
      "type": "string",
      "required": true
    }
  }
}
